<div class="kt-grid kt-grid--ver kt-grid--root">
    <div class="kt-grid__item   kt-grid__item--fluid kt-grid  kt-grid kt-grid--hor kt-login-v2" id="kt_login_v2">
        <!--begin::Item-->
        <div class="kt-grid__item  kt-grid--hor">
            <!--begin::Heade-->
            <div class="kt-login-v2__head">
                <div class="kt-login-v2__logo">
                    <a href="#">
                        <img src="{{ asset("media/logos/logo-5.png") }}" alt="Login pic" />
                    </a>
                </div>
            </div>
            <!--begin::Head-->
        </div>
        <!--end::Item-->

        <!--begin::Item-->
        <div class="kt-grid__item  kt-grid  kt-grid--ver  kt-grid__item--fluid">
            <!--begin::Body-->
            <div class="kt-login-v2__body">
                <!--begin::Wrapper-->
                <div class="kt-login-v2__wrapper">
                    <div class="kt-login-v2__container">
                        <div class="kt-login-v2__title">
                            <h3>Connectez vous <b>SymfAgents</b> </h3>
                        </div>

                        <!--begin::Form-->
                        <form class="kt-login-v2__form kt-form" method="post" autocomplete="off" id="kt_login_form">
{#                            <div class="form-group">#}
{#                                <input class="form-control" type="text" placeholder="Username" name="username" autocomplete="off">#}
{#                            </div>#}
{#                            <div class="form-group">#}
{#                                <input class="form-control" type="password" placeholder="Password" name="password" autocomplete="off">#}
{#                            </div>#}
                            {% if app.user %}
                                <div class="mb-3">
                                    You are logged in as {{ app.user.username }}, <a href="{{ path('app_logout') }}">Logout</a>
                                </div>
                            {% endif %}
                            <div class="form-group">
                                <input type="email" value="{{ last_username }}" name="email" id="inputEmail" placeholder="Votre email" class="form-control" required autofocus>
                            </div>
                            <div class="form-group">
                                <input type="password" name="password" id="inputPassword" class="form-control" placeholder="Votre mot de passe" required>
                            </div>
                            <input type="hidden" name="_csrf_token"
                                   value="{{ csrf_token('authenticate') }}"
                            >

                            {#
                                Uncomment this section and add a remember_me option below your firewall to activate remember me functionality.
                                See https://symfony.com/doc/current/security/remember_me.html

                                <div class="checkbox mb-3">
                                    <label>
                                        <input type="checkbox" name="_remember_me"> Remember me
                                    </label>
                                </div>
                            #}
                            {% if error %}
                                <div class="mt-4 alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                            {% endif %}
                            <!--begin::Action-->
                            <div class="kt-login-v2__actions">
                                <a href="#" class="kt-link kt-link--brand">
                                    Forgot Password ?
                                </a>
                                <button type="submit" class="btn btn-brand btn-elevate btn-pill" id="kt_login_submit">Connectez vous </button>
                            </div>
                            <!--end::Action-->
                        </form>
                        <!--end::Form-->

